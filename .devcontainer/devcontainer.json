{
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.vscode-json",
        "ms-vscode.vscode-html-language-features",
        "wholroyd.jinja"
      ],
      "settings": {
        "files.watcherExclude": {
          "**/__pycache__/**": true,
          "**/.git/objects/**": true,
          "**/.git/subtree-cache/**": true,
          "**/*.pyc": true,
          "**/node_modules/*/**": true
        }
      }
    }
  },
  "dockerComposeFile": "../docker-compose.yml",
  "name": "Flask Development",
  "service": "flask",
  "workspaceFolder": "/app"
}